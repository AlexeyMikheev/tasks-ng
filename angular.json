{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "module1": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/module1",
      "sourceRoot": "projects/module1/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/module1",
            "index": "projects/module1/src/index.html",
            "main": "projects/module1/src/main.ts",
            "polyfills": "projects/module1/src/polyfills.ts",
            "tsConfig": "projects/module1/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/module1/src/favicon.ico",
              "projects/module1/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/module1/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/module1/src/environments/environment.ts",
                  "with": "projects/module1/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "module1:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "module1:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "module1:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/module1/src/test.ts",
            "polyfills": "projects/module1/src/polyfills.ts",
            "tsConfig": "projects/module1/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "projects/module1/src/favicon.ico",
              "projects/module1/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/module1/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/module1/tsconfig.app.json",
              "projects/module1/tsconfig.spec.json",
              "projects/module1/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "module2": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/module2",
      "sourceRoot": "projects/module2/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/module2",
            "index": "projects/module2/src/index.html",
            "main": "projects/module2/src/main.ts",
            "polyfills": "projects/module2/src/polyfills.ts",
            "tsConfig": "projects/module2/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/module2/src/favicon.ico",
              "projects/module2/src/assets"
            ],
            "styles": [
              "projects/module2/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/module2/src/environments/environment.ts",
                  "with": "projects/module2/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "module2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "module2:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "module2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/module2/src/test.ts",
            "polyfills": "projects/module2/src/polyfills.ts",
            "tsConfig": "projects/module2/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "projects/module2/src/favicon.ico",
              "projects/module2/src/assets"
            ],
            "styles": [
              "projects/module2/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/module2/tsconfig.app.json",
              "projects/module2/tsconfig.spec.json",
              "projects/module2/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "module1",
  "cli": {
    "analytics": false
  }
}